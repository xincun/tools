[Macro definition]
%name%=@E proc/method/class name:
%autor%=@E author name:
%args%=@E arguments:
%desc%=@E description:
[proc | procedure tcl]
# ############################
# Procedure: 	%name%
# Author: 	 	%autor%
#
# description:	%desc%
#
# example:	 	%name% %args%
proc %name% {%args%} {
	set fcFun "%name%"
}
[method | method tcl]
# ############################
# method: %name%
# Author: 	 %autor%
#
# description:	%desc%
#
# example:	 	%name% %args%
method %name% {%args%} {

}
[class | class tcl]
# ############################
# class: %name%
# Author: 	 %autor%
#
# description:	%desc%
#
# example:	 	%name% %args%
class %name% {

}

